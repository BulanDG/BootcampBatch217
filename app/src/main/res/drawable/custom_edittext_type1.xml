<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--Membentuk sudut tumpul di keempat sisi-->
    <corners android:radius="15dp"/>

    <!--warna dasar shape-->
    <solid android:color="#FFFFFF" />

    <padding
        android:left="10dp"
        android:right="10dp"
        android:bottom="10dp"
        android:top="10dp"/>

    <!--warna border-->
    <stroke
        android:color="#100387"
        android:width="3dp"/>



</shape>